{
    "git": {
      "commitMessage": "chore: release v${version}",
      "tagName": "v${version}",
      "requireBranch": "next"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true,
      "releaseName": "v${version}"
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
}  